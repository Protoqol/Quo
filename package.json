    "autoprefixer": "^10.4.7",
    "electron": "^19.0.1",
    "electron-reloader": "^1.2.3",
    "laravel-mix": "^6.0.44",
    "mix-tailwindcss": "^1.3.0",
    "postcss": "^8.4.14",
    "resolve-url-loader": "^5.0.0",
